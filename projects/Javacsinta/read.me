



------------------------------------------------------
order_detail VIEW "tesztelése"


SELECT
order_detail_pancake_id,
order_detail_date,
SUM(order_detail_amount)
FROM
order_detail
WHERE ( 1 = 1 )
AND ( order_detail_customer_id = 1 )
GROUP BY
order_detail_date,
order_detail_pancake_id
ORDER BY
order_detail_date,
order_detail_pancake_id